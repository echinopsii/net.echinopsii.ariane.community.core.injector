<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

    <managed-bean>
        <managed-bean-name>injectorLayoutController</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.core.injector.wat.controller.InjectorLayoutController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>injectorDashboardController</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.core.injector.wat.controller.InjectorDashboardController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>injectorMenuController</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.core.injector.wat.controller.InjectorMenuController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>injectorBreadCrumController</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.core.injector.wat.controller.InjectorBreadCrumController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

    <managed-bean>
        <managed-bean-name>injectorExternalTreeMenuEntity</managed-bean-name>
        <managed-bean-class>net.echinopsii.ariane.community.core.injector.wat.controller.InjectorExternalTreeMenuEntity</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>

    <managed-bean>
