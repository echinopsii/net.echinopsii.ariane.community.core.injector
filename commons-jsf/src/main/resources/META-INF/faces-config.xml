<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">

    <managed-bean>
        <managed-bean-name>injectorsLayoutController</managed-bean-name>
        <managed-bean-class>com.spectral.cc.core.injector.commons.controller.InjectorLayoutController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>injectorsMenuController</managed-bean-name>
        <managed-bean-class>com.spectral.cc.core.injector.commons.controller.InjectorMenuController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>injectorsBreadCrumController</managed-bean-name>
        <managed-bean-class>com.spectral.cc.core.injector.commons.controller.InjectorBreadCrumController</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>

</faces-config>
